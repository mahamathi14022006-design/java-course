import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String args[]) {
       Scanner s=new Scanner(System.in);
        int n,i;
        n=s.nextInt();
       // n=s.nextInt();
        for(i=1;i<=n;i++)
        {
            System.out.print(i+" ");   
        }
    }
    
}
