import java.io.*;
import java.util.*;

public class Solution {

   public int java(int a,int b)
    {
     int c=a+b;
       return c;
       //System.out.println(c);
    }

    public static void main(String[] args) {
        Solution ob=new Solution();
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
         int c=ob.java(a,b);
    System.out.println(c);
    }
}


   
